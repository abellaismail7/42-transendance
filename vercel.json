{
  "version": 2,
  "builds": [
    {
      "src": "apps/backend/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "apps/backend/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "(.*)",
      "dest": "/apps/web/.next/server/pages",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
